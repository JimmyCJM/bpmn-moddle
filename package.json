{
  "name": "bpmn-moddle",
  "version": "0.14.0",
  "description": "A moddle wrapper for BPMN 2.0",
  "scripts": {
    "all": "run-s lint test bundle",
    "bundle": "NODE_ENV=production webpack",
    "dev": "npm test -- --watch",
    "lint": "eslint 'lib/**/*.js' 'test/**/*.js'",
    "prepublishOnly": "run-s bundle",
    "test": "mocha --full-trace"
  },
  "module": "lib/index.js",
  "main": "dist/bundle.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bpmn-io/bpmn-moddle"
  },
  "keywords": [
    "bpmn",
    "moddle",
    "bpmn20",
    "meta-model"
  ],
  "author": {
    "name": "Nico Rehwaldt",
    "url": "https://github.com/Nikku"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cmof-parser": "^0.2.0",
    "eslint": "^4.9.0",
    "eslint-plugin-mocha": "^4.11.0",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.1",
    "source-map-support": "^0.5.0",
    "webpack": "^3.6.0",
    "xsd-schema-validator": "^0.5.0"
  },
  "dependencies": {
    "min-dash": "^1.0.0",
    "moddle": "github:bpmn-io/moddle#ES2015",
    "moddle-xml": "github:bpmn-io/moddle-xml#ES2015"
  }
}
